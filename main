package javaforme;

import java.math.BigInteger;

public class function {
    public static void main(String[] args) {

        System.out.println(fuctorial(20));

    }
    public static long fuctorial(long x) {
        if (x == 1) {
            return 1;
        }

        return x * fuctorial(x - 1);
    }


}
